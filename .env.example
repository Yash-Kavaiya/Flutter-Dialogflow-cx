# Dialogflow CX Configuration
# Copy this file to .env and fill in your actual values

# Google Cloud Project ID
PROJECT_ID=your-project-id

# Dialogflow CX Location (e.g., us-central1, europe-west1, asia-northeast1)
LOCATION_ID=us-central1

# Dialogflow CX Agent ID
AGENT_ID=your-agent-id

# Language Code (e.g., en, es, fr, de, ja, zh)
LANGUAGE_CODE=en

# Optional: Path to service account credentials JSON file
CREDENTIALS_PATH=path/to/credentials.json

# Optional: Enable debug logging (true/false)
ENABLE_LOGGING=true
